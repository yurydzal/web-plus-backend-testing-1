      "@nestjs/cli": "^8.0.0",
      "@nestjs/schematics": "^8.0.0",
      "@nestjs/testing": "^8.0.0",
      "@stryker-mutator/core": "^6.2.2",
      "@stryker-mutator/jest-runner": "^6.2.2",
      "@types/express": "^4.17.13",
      "@types/jest": "27.4.1",
      "@types/node": "^16.0.0",
